{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "outDir": "./dist",
    "lib": ["DOM", "ES5", "ES2015.Collection", "ES2015.Promise"],
    "types": [],
    "baseUrl": "./node_modules",
    "jsx": "preserve",
    "typeRoots": ["node_modules/@types"],
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
